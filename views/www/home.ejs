<!doctype html>
<html lang="en">
    <%- include("partials/head") %>

    <body>
        <%- include("partials/navbar") %>

        <section id="hero">
            <div
                id="heroCarousel"
                class="carousel slide carousel-fade"
                data-ride="carousel"
