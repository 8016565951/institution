            <script>
                function toggleSwitch() {
                    const switchInput = document.getElementById('flexSwitchCheckDefault');
                    const switchLabel = document.getElementById('switchLabel');

                    if (switchInput.checked) {
                        switchLabel.textContent = 'Inctive';
                        switchLabel.classList.remove('active-label');
                        switchLabel.classList.add('Inactive-label');
                    } else {
                        switchLabel.textContent = 'Active';
                        switchLabel.classList.remove('inactive-label');
                        switchLabel.classList.add('active-label');
                    }
                }
            </script>
